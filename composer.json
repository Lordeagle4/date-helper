{
  "name": "awtechs/date-helper",
  "description": "A timezone-aware, business-week-friendly Laravel date helper with Blade directives and components.",
  "keywords": [
    "laravel",
    "date",
    "carbon",
    "timezone",
    "business",
    "helper"
  ],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Michael Ugochukwu",
      "email": "eaglemike7@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.0",
    "illuminate/support": "^9.0|^10.0|^11.0|^12.0",
    "nesbot/carbon": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "Awtechs\\DateHelper\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Awtechs\\DateHelper\\DateHelperServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}